{
  "name": "movies-review",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "migrate": "knex migrate:latest --knexfile models/knexfile.js",
